<h4>Страница логина</h4>
<form>
    <div class="form-group">
        <label>Логин</label>
        {{input type="text" placeholder="Логин" class="form-control" value=model.userName}}
    </div>
    <div class="form-group">
        <label>Пароль</label>
        {{input type="password" placeholder="Пароль" class="form-control" value=model.password}}   
    </div>
    <button type="button" class="btn btn-primary" {{action "login"}}>Логин</button>
</form>


